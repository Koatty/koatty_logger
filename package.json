{
  "name": "koatty_logger",
  "version": "2.4.2",
  "description": "Logger for koatty.",
  "scripts": {
    "build": "npm run build:js && npm run build:dts && npm run build:doc && npm run build:cp",
    "build:cp": "node scripts/postBuild && copyfiles package.json LICENSE README.md dist/",
    "build:js": "npx rollup --bundleConfigAsCjs -c .rollup.config.js",
    "build:doc": "npx api-documenter markdown --input temp --output docs/api",
    "build:dts": "bash ../../scripts/build-dts.sh",
    "eslint": "eslint --ext .ts,.js ./",
    "lock": "npm i --package-lock-only",
    "prepublishOnly": "npm test && npm run build && git push --follow-tags origin",
    "prerelease": "npm test && npm run build",
    "test": "npm run eslint && jest --passWithNoTests",
    "test:cov": "jest --collectCoverage --detectOpenHandles"
  },
  "main": "./dist/index.js",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/index.mjs",
    "types": "./dist/index.d.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koatty/koatty_logger.git"
  },
  "keywords": [
    "web",
    "typescript",
    "framework",
    "mvc",
    "koa2",
    "restful",
    "agile",
    "koatty_logger",
    "koatty"
  ],
  "engines": {
    "node": ">10.0.0"
  },
  "author": {
    "name": "richenlin",
    "email": "richenlin@gmail.com"
  },
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/koatty/koatty_logger/issues"
  },
  "homepage": "https://github.com/koatty/koatty_logger",
  "maintainers": [
    {
      "name": "richenlin",
      "email": "richenlin@gmail.com"
    }
  ],
  "devDependencies": {
    "@commitlint/cli": "^19.x.x",
    "@commitlint/config-conventional": "^19.x.x",
    "@microsoft/api-documenter": "^7.x.x",
    "@microsoft/api-extractor": "^7.x.x",
    "@rollup/plugin-commonjs": "^28.x.x",
    "@rollup/plugin-json": "^6.x.x",
    "@rollup/plugin-node-resolve": "^15.x.x",
    "@types/jest": "^29.x.x",
    "@types/koa": "^2.x.x",
    "@types/lodash": "^4.x.x",
    "@types/node": "^22.x.x",
    "@typescript-eslint/eslint-plugin": "^8.x.x",
    "@typescript-eslint/parser": "^8.x.x",
    "conventional-changelog-cli": "^5.x.x",
    "copyfiles": "^2.x.x",
    "eslint": "^8.x.x",
    "eslint-plugin-jest": "^28.x.x",
    "husky": "^4.x.x",
    "jest": "^29.x.x",
    "jest-html-reporters": "^3.x.x",
    "reflect-metadata": "^0.x.x",
    "rollup": "^4.x.x",
    "rollup-plugin-delete": "^2.1.0",
    "rollup-plugin-typescript2": "^0.x.x",
    "standard-version": "^9.x.x",
    "ts-jest": "^29.x.x",
    "ts-node": "^10.x.x",
    "tslib": "^2.x.x",
    "typescript": "^5.x.x"
  },
  "dependencies": {
    "koatty_lib": "workspace:*",
    "tslib": "^2.8.1",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "peerDependencies": {
    "koatty_lib": "^1.4.6",
    "tslib": "^2.x.x"
  }
}
