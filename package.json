{
  "name": "koatty_logger",
  "version": "2.8.3",
  "description": "Logger for koatty.",
  "scripts": {
    "build": "pnpm run build:js && pnpm run build:dts && pnpm run build:doc && pnpm run build:cp",
    "build:js": "tsup",
    "build:dts": "bash ../../scripts/build-dts.sh",
    "build:doc": "api-documenter markdown --input temp --output docs/api",
    "build:cp": "node ../../scripts/postBuild.js",
    "clean": "rimraf dist temp docs/api",
    "lint": "eslint --ext .ts,.js ./src",
    "test": "pnpm run lint && jest --passWithNoTests",
    "test:cov": "jest --collectCoverage --detectOpenHandles"
  },
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koatty/koatty_logger.git"
  },
  "keywords": [
    "web",
    "typescript",
    "framework",
    "mvc",
    "koa2",
    "restful",
    "agile",
    "koatty_logger",
    "koatty"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "author": {
    "name": "richenlin",
    "email": "richenlin@gmail.com"
  },
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/koatty/koatty_logger/issues"
  },
  "homepage": "https://github.com/koatty/koatty_logger",
  "maintainers": [
    {
      "name": "richenlin",
      "email": "richenlin@gmail.com"
    }
  ],
  "devDependencies": {
    "@microsoft/api-documenter": "^7.26.x",
    "@microsoft/api-extractor": "^7.52.x",
    "@types/jest": "^29.5.x",
    "@types/koa": "^2.x.x",
    "@types/lodash": "^4.x.x",
    "@types/node": "^22.x.x",
    "@typescript-eslint/eslint-plugin": "^8.x.x",
    "@typescript-eslint/parser": "^8.x.x",
    "eslint": "^8.x.x",
    "eslint-plugin-jest": "^28.x.x",
    "jest": "^29.x.x",
    "jest-html-reporters": "^3.x.x",
    "reflect-metadata": "^0.2.2",
    "ts-jest": "^29.x.x",
    "tslib": "^2.x.x"
  },
  "dependencies": {
    "koatty_lib": "workspace:*",
    "tslib": "^2.8.1",
    "winston": "^3.19.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "peerDependencies": {
    "koatty_lib": "^1.4.7",
    "tslib": "^2.x.x"
  },
  "types": "./dist/index.d.ts"
}
